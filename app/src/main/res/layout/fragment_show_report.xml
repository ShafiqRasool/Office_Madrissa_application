<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="viewModel"
            type="com.example.useapiwithretrofit.report.showreportlist.FragmentShowReportViewModel" />
    </data>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".report.showreportlist.FragmentShowReport">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_show_report"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:SetCustomAdapter="@{viewModel.adapter}"
        tools:listitem="@layout/card_view_operations_report" />

</androidx.constraintlayout.widget.ConstraintLayout>
</layout>
